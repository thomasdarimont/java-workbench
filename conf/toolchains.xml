<?xml version="1.0" encoding="UTF8"?>
<toolchains>

    <!-- JDK toolchains -->
    <toolchain>
        <type>jdk</type>
        <provides>
            <version>1.8</version>
            <vendor>OpenJDK</vendor>
            <os>Windows 10</os>
        </provides>
